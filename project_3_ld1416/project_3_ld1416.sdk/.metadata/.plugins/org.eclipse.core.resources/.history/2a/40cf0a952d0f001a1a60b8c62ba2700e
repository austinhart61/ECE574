/*
 *
 * This application configures UART 16550 to baud rate 9600.
 * PS7 UART (Zynq) is not initialized by this application, since
 * bootrom/bsp configures it to baud rate 115200
 *
 * ------------------------------------------------
 * | UART TYPE   BAUD RATE                        |
 * ------------------------------------------------
 *   uartns550   9600
 *   uartlite    Configurable only in HW design
 *   ps7_uart    115200 (configured by bootrom/bsp)
 */

#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"
#include "xiomodule.h"
#include "xparameters.h"


int main()
{
    init_platform();

    u32 data, chars01, chars02;

    XIOModule iomodule; // iomodule variable for gpi, gpo, and uart

    // Initialize module to obtain base address
	data = XIOModule_Initialize(&iomodule, XPAR_IOMODULE_0_DEVICE_ID);
	data = XIOModule_Start(&iomodule);

	while(1){
		chars01 = XIOModule_DiscreteRead(&iomodule, 1);
		chars02 = XIOModule_DiscreteRead(&iomodule, 2);
	}

    cleanup_platform();
    return 0;
}
